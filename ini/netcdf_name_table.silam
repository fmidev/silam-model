time_label_position = instant

var = go3 volume_mixing_ratio XXX -1 O3 -1 -1 0.6035 0
var = co volume_mixing_ratio XXX -1 CO -1 -1 1.0346 0
var = hcho volume_mixing_ratio XXX -1 HCHO -1 -1 0.9657 0
