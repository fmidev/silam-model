  # Internal olefin carbon bond in CB05 (R-C=C-R)
  chemical_name = IOLE
  molar_mass = 56.1 g/mole #For cis-2-butene and trans-2-butene
  low_concentration_threshold = 1 pg/m3
  can_be_aerosol = NO
  can_be_gas = YES

  Henry_constant_at_298K    =  5.2e-8    # mol/kg/Pa  #Mean of cis- and trans-2-buten values by Irmann (1965) 
  Henry_constant_T_dependence_exponent_scale = 0.0  #Dimension -- Degrees Kelvin
  Wesely_f0 = 0.0 

  gas_surf_resistance_over_water = 2000.    #copy of OLE
  gas_surf_resistance_land_default = 1000.  #copy of OLE
  gas_molecular_diffusivity_in_air   = 8e-6
  gas_molecular_diffusivity_in_water = 6e-10
  washout_coef_rain_ratio_to_SILAM_standard = 1.0e-3  #copy of OLE
  washout_coef_snow_ratio_to_SILAM_standard = 1.0e-3  #copy of OLE
  
END_LIST = substance

# ***********************************************************************

LIST = substance
  # Isoprene products (methacrolein C4H6O, methyl vinyl ketone, etc)
  chemical_name = ISPD
  molar_mass = 70.09 g/mole
  low_concentration_threshold = 1 pg/m3
  can_be_aerosol = NO
  can_be_gas = YES

  # Henry for methacrolein      : 4.8e-5 mol/kg/Pa and 4300 K, Ji and Evans (2007) 
  # Henry for methyl viny ketone: 2.6e-4 mol/kg/Pa and 4800 K, Ji and Evans (2007) 
  Henry_constant_at_298K    =  4.8e-5    # mol/kg/Pa   #methacrolein
  Henry_constant_T_dependence_exponent_scale = 4300.0  #Dimension -- Degrees Kelvin
  Wesely_f0 = 1.0 #RH2019: Karl et al., Science 2010.

  gas_surf_resistance_over_water = 2500.    #CHECK, copy of C5H8 (isoprene)
  gas_surf_resistance_land_default = 500.  #CHECK, copy of C5H8 (isoprene)
  gas_molecular_diffusivity_in_air   = 8e-6
  gas_molecular_diffusivity_in_water = 6e-10
  washout_coef_rain_ratio_to_SILAM_standard = 1.0e-1  #CHECK, copy of C5H8
  washout_coef_snow_ratio_to_SILAM_standard = 1.0e-1  #CHECK, copy of C5H8
  
END_LIST = substance

# ***********************************************************************

LIST = substance
  # Sesquiterpenes
  chemical_name = SESQ
  molar_mass = 204.35 g/mole #Using basic formula C15H24, Simplest C15H24O with 220.35
  low_concentration_threshold = 1 pg/m3
  can_be_aerosol = NO
  can_be_gas = YES

  Henry_constant_at_298K    =  4.0e-7    # mol/kg/Pa  #CHECK, SOME ESTIMATE (some isoprenes)
  Henry_constant_T_dependence_exponent_scale = 0.0  #Dimension -- Degrees Kelvin
  Wesely_f0 = 0.0 

  gas_surf_resistance_over_water = 2500.    #CHECK, copy of C5H8_2 (monoterpine)
  gas_surf_resistance_land_default = 50.    #CHECK, copy of C5H8_2 (monoterpine)
  gas_molecular_diffusivity_in_air   = 8e-6
  gas_molecular_diffusivity_in_water = 6e-10
  washout_coef_rain_ratio_to_SILAM_standard = 1.0  #CHECK, copy of C5H8_2 (monoterpine)
  washout_coef_snow_ratio_to_SILAM_standard = 1.0  #CHECK, copy of C5H8_2 (monoterpine)
  
END_LIST = substance

