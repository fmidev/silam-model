LIST = substance
  chemical_name = HNO3
  GRIB2_constituent_type = 17
  molar_mass = 63. g/mole
  low_concentration_threshold = 0.1 pg/m3 #R.H. Decreased by factor of 1000: HNO3
  number_of_reference_subst_in_mixture = 1 
  names_of_ref_subst_in_mixture = soluble
  fractions_of_ref_subst_in_mixture = 1
  can_be_aerosol = YES
  can_be_gas = YES
  
  # Use the effective Henry constant which is the standard Henry constant times (1+10^(pH-pKa)) 
  # where pKa=-log(Ka) and Ka is the the acid constant Ka = K_eq*[H20]=[H+][NO3-]/[HNO3]. For HNO3, kPa = -1.37.
  # Using pH = 8.1 (for seawater), one obtains a multiplication factor of about 3.0e9
  # Henry_constant_at_298K    =  2.1    # mol/kg/pa
  Henry_constant_at_298K    =  6.2e9    # mol/kg/pa #effective one
  Henry_constant_T_dependence_exponent_scale = 8700.  #Dimension -- Degrees Kelvin
  #Dissoc_coeff = 0. # Mol/kg
  Wesely_f0 = 0. 
  
  gas_surf_resistance_over_water = 100.   #RH2018: update. Seinfeld and Pandis: Atm. Chem. & Phys.  OLD value: 25.
  gas_surf_resistance_land_default = 25.
  gas_molecular_diffusivity_in_air   = 1.16e-5 #RH2018: update. Seinfeld and Pandis: Atm. Chem. & Phys.: D=D_H2O/1.87 = 2.178e-5/1.87
  gas_molecular_diffusivity_in_water = 6e-10
  washout_coef_rain_ratio_to_SILAM_standard = 10.0
  washout_coef_snow_ratio_to_SILAM_standard = 10.0

  dry_particle_density_kg_per_m3 = 1512.9      #RH2017: update. Wikipedia etc. Old value 1350. was NAT density.
  humidity_growth_scale_alpha = 1.0
  humidity_growth_curvature_beta = 1.0
  deliquescence_humidity_0_to_1 = 1.  ! enough to exclude growth

END_LIST = substance

