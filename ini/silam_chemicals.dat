LIST = substance
  # benzene C6H6
  chemical_name = BENZENE
  molar_mass = 78.11 g/mole
  low_concentration_threshold = 1 pg/m3
  can_be_aerosol = NO
  can_be_gas = YES

  Henry_constant_at_298K    =  1.7e-6    # mol/kg/Pa  #Staudinger and Roberts (2001)
  Henry_constant_T_dependence_exponent_scale = 4200.0  #Dimension -- Degrees Kelvin
  Wesely_f0 = 0.0 

  gas_surf_resistance_over_water = 2000.    #CHECK, copy of TOL
  gas_surf_resistance_land_default = 1000.  #CHECK, copy of TOL
  gas_molecular_diffusivity_in_air   = 8e-6
  gas_molecular_diffusivity_in_water = 6e-10
  washout_coef_rain_ratio_to_SILAM_standard = 1.0e-2  #CHECK, copy of TOL
  washout_coef_snow_ratio_to_SILAM_standard = 1.0e-2  #CHECK, copy of TOL
  
END_LIST = substance

